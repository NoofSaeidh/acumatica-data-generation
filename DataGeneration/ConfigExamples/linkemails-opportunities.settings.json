{
  "$type": "DataGeneration.Entities.Emails.LinkEmailsGenerationSettings, DataGeneration",
  "#comment": "Create emails, fetch entities and link emails to those entities.",
  "GenerationEntity": "Email",
  "SearchPattern": {
    "EntityType": "DataGeneration.Soap.Opportunity",
    "CreatedDate": {
      "Condition": "IsGreaterThanOrEqualsTo",
      "Value": "2018-10-1"
    },
    "LinqPattern": {
      "Reverse": true,
      "Skip": 0,
      "Take": 100
    }
  },
  "RandomizerSettings": {
    "PxTypeForLinkedEntity": "PX.Objects.CR.CROpportunity",
    "DateRange": [
      "2018-01-02",
      "2018-02-03"
    ],
    "SystemEmailAddress": "testadmin@acumatica.com",
    "EmailCountPerEntity": [
      {
        "Probability": 0.9,
        "Value": [ 1, 5 ]
      }
    ]
  }
}