<Endpoint xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="datagen" version="18.200.001" systemContractVersion="3" xmlns="http://www.acumatica.com/entity/maintenance/5.31">
  <ExtendsEndpoint name="Default" version="18.200.001" />
  <TopLevelEntity name="Lead" screen="CR301000">
    <Fields>
      <Field name="CreatedDate" type="DateTimeValue" />
      <Field name="NoteID" type="GuidValue" />
    </Fields>
    <Mappings>
      <Mapping field="NoteID">
        <To object="Lead" field="NoteID" />
      </Mapping>
      <Mapping field="CreatedDate">
        <To object="Lead" field="CreatedDateTime" />
      </Mapping>
    </Mappings>
    <Actions>
      <Action name="Disqualify" mappedTo="Disqualify">
        <Fields>
          <Field name="Reason" type="StringValue" />
        </Fields>
        <Mappings>
          <Mapping field="Reason">
            <To object="FilterPreview" field="Reason" />
          </Mapping>
        </Mappings>
      </Action>
      <Action name="ConvertToOpportunity" mappedTo="convertToOpportunityAll">
        <Fields>
          <Field name="Class" type="StringValue" />
          <Field name="CloseDate" type="DateTimeValue" />
          <Field name="Subject" type="StringValue" />
        </Fields>
        <Mappings>
          <Mapping field="Subject">
            <To object="OpportunityInfo" field="Subject" />
          </Mapping>
          <Mapping field="Class">
            <To object="OpportunityInfo" field="OpportunityClass" />
          </Mapping>
          <Mapping field="CloseDate">
            <To object="OpportunityInfo" field="CloseDate" />
          </Mapping>
        </Mappings>
      </Action>
    </Actions>
  </TopLevelEntity>
  <TopLevelEntity name="Opportunity" screen="CR304000">
    <Fields>
      <Field name="CreatedDate" type="DateTimeValue" />
      <Field name="NoteID" type="GuidValue" />
    </Fields>
    <Mappings>
      <Mapping field="NoteID">
        <To object="Opportunity" field="NoteID" />
      </Mapping>
      <Mapping field="CreatedDate">
        <To object="Opportunity" field="CreatedDateTime" />
      </Mapping>
    </Mappings>
  </TopLevelEntity>
  <TopLevelEntity name="Case" screen="CR306000">
    <Fields>
      <Field name="CreatedDate" type="DateTimeValue" />
      <Field name="NoteID" type="GuidValue" />
    </Fields>
    <Mappings>
      <Mapping field="NoteID">
        <To object="Case" field="NoteID" />
      </Mapping>
      <Mapping field="CreatedDate">
        <To object="Case" field="CreatedDateTime" />
      </Mapping>
    </Mappings>
  </TopLevelEntity>
  <TopLevelEntity name="Email" screen="CR306015">
    <Fields>
      <Field name="NoteID" type="GuidValue" />
    </Fields>
    <Mappings>
      <Mapping field="NoteID">
        <To object="Message" field="NoteID" />
      </Mapping>
      <Mapping field="Date">
        <To object="CurrentMessage: 1" field="StartDate" />
      </Mapping>
    </Mappings>
  </TopLevelEntity>
  <TopLevelEntity name="Campaign" screen="CR202000">
    <Fields>
      <Field name="CampaignID" type="StringValue" />
      <Field name="Members" type="CampaignMember[]" />
    </Fields>
    <Mappings>
      <Mapping field="CampaignID">
        <To object="Campaign" field="CampaignID" />
      </Mapping>
      <Mapping field="Members">
        <Mapping field="ContactID">
          <To object="CampaignMembers" field="ContactID" />
        </Mapping>
        <Mapping field="Type">
          <To object="CampaignMembers" field="Contact__ContactType" />
        </Mapping>
        <Mapping field="CampaignID">
          <To object="CampaignMembers" field="CampaignID" />
        </Mapping>
      </Mapping>
    </Mappings>
  </TopLevelEntity>
  <Detail name="CampaignMember">
    <Fields>
      <Field name="CampaignID" type="StringValue" />
      <Field name="ContactID" type="IntValue" />
      <Field name="Type" type="StringValue" />
    </Fields>
  </Detail>
</Endpoint>